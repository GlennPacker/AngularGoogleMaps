
    <script src="https://maps.googleapis.com/maps/api/js"></script>
    <script>
        function Initialize() {
            var mapOptions = {
                center: new google.maps.LatLng(50.87, 0.25),
                zoom: 14
            };
            var gmap = new google.maps.Map(document.getElementById('mapdiv'), mapOptions);

            var lineCoords =
                [
                    new google.maps.LatLng(50.87, 0.25),
                    new google.maps.LatLng(50.875, 0.255),
                    new google.maps.LatLng(50.878, 0.255),
                    new google.maps.LatLng(50.880, 0.256),
                ]
            var apath = new google.maps.Polyline({
                path: lineCoords,
                strokeColor: '#0000aa',
                strokeOpacity: .8,
                strokeWeight: 3
            });

            apath.setMap(gmap);
        };

        google.maps.event.addDomListener(window, "load", Initialize);
    </script>

    <div id="mapdiv" style="height:400px;"></div>

